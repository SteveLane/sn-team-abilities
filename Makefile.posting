	Rscript --no-save --no-restore -e \
		"knitr::knit('$(<F)', output = '$(subst Rmd,md,$(<F))')"; \
	sed -i.bk 's/sn-assets\//\/sn-assets\//g' $(subst Rmd,md,$(<F)); \
	mv $(subst Rmd,md,$(<F)) \
		~/github/website/content/post/2018-08-14-round15-wrapup.md; \
	touch .round15-wrapup

