manuscripts/round17-2018-analysis.html: Rmd/round17-2018-analysis.Rmd
	cd $(<D); \
	Rscript --no-save --no-restore -e \
		"rmarkdown::render('$(<F)', output_dir = '../manuscripts/')"; \
	mkdir -p ~/github/website/static/sn-assets/round17-2018; \
	cp -R sn-assets/round17-2018/ ~/github/website/static/sn-assets/round17-2018/

