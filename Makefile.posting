################################################################################
manuscripts/introduction-analysis.html: Rmd/introduction-analysis.Rmd
	cd $(<D); \
	Rscript --no-save --no-restore -e \
		"rmarkdown::render('$(<F)', output_dir = '../manuscripts/')"; \
	mkdir -p ~/github/website/static/sn-assets/intro-2018; \
	cp -R sn-assets/intro-2018/ ~/github/website/static/sn-assets/intro-2018/

Rmd/.intro: Rmd/introduction.Rmd
	cd $(<D); \
	Rscript --no-save --no-restore -e \
		"knitr::knit('$(<F)', output = '$(subst Rmd,md,$(<F))')"; \
	sed -i.bk 's/sn-assets\//\/sn-assets\//g' $(subst Rmd,md,$(<F)); \
	mv $(subst Rmd,md,$(<F)) \
		~/github/website/content/post/2018-04-22-introduction.md; \
	touch .intro

